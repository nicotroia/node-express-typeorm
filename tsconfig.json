{
    "version": "3.7.2",
    "compilerOptions": {
        "lib": ["es5", "es6"],
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "strict": true,
        "alwaysStrict": true,
        "noEmitOnError": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "sourceMap": false,
        "watch": false,
        "baseUrl": "."
    },
    "exclude": [
        "dist",
        "init",
        "node_modules"
    ]
}
