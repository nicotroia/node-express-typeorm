{
  "name": "node-express-typeorm",
  "version": "0.0.1",
  "description": "Node + Express + TypeORM",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "fixtures": "fixtures ./fixtures --config ./dist/typeorm.config.js --sync",
    "start": "npm install && npm run build && npm run fixtures && node dist/index.js",
    "dev": "NODE_ENV=develop tsc-watch --onSuccess 'node ./dist/index.js' --onFailure 'echo Oops! Compilation Failed' --compiler typescript/bin/tsc"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pg": "^7.14.0",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "tsc-watch": "^4.0.0",
    "typeorm-fixtures-cli": "^1.3.7",
    "typescript": "^3.7.2"
  }
}
